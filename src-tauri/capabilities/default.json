{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-overlay-icon",
    "notification:default",
    "updater:default",
    "process:allow-restart"
  ]
}
